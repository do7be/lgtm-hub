{
  "presets": [
    [
      "@babel/preset-react", {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
    [
      "@babel/preset-env", {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    ["transform-class-properties"],
    [
      "@babel/plugin-transform-runtime", {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
